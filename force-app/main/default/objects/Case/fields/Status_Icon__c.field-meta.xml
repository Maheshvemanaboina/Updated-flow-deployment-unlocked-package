<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_Icon__c</fullName>
    <deprecated>false</deprecated>
    <description>(BGI) (DE) This formula field is used to display the red flag icon when the status of case is not Closed.Used in Related List,Page Layout and List Views.</description>
    <externalId>false</externalId>
    <formula>IF(
NOT(IsClosed), 
IMAGE(&quot;/resource/BGI_Icons/red_flag.png&quot;, &quot;red&quot;), 
&quot;&quot;)</formula>
    <label>Status Icon</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
